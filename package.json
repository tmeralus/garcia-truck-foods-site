{
  "name": "gatsby-contentful-blog-portfolio",
  "description": "Gatsby starter for a blog-portfolio served by Contentful",
  "author": "escapemanuele",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
