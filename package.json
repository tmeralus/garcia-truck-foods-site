    "moment": "^2.24.0",
    "node-sass": "^4.12.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1",
    "react-icons": "^3.7.0",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@cypress/webpack-preprocessor": "^4.1.1",
    "@testing-library/cypress": "^5.3.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.4.0",
    "babel-loader": "^8.0.6",
